{
    "name": "engbz/filemanager",
    "description": "File manager package (fork of mwguerra/filemanager)",
    "license": "MIT",

    "require": {
        "php": "^8.2",
        "filament/filament": "^5.0",
        "illuminate/contracts": "^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },

    "autoload": {
        "psr-4": {
            "Engbz\\Filemanager\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Engbz\\Filemanager\\FilemanagerServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
